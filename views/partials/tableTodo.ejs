<table class="table ">
    <thead>
        <tr>
            <th scope="col">No</th>
            <th scope="col">Todo</th>
            <th scope="col">Dibuat Pada</th>
            <th scope="col">Deadline</th>
            <th scope="col">Aksi</th>
        </tr>
    </thead>
    <tbody>
        <% result.forEach(datas=> { %>
            <tr>
                <th scope="row">1</th>
                <td>
                    <%= datas.todo %>
                </td>
                <td>
                    <%= datas.createdAt %>
                </td>
                <td>
                    <%= datas.deadline %>
                </td>
                <td>
                    <a href="/mytodo/delete"><button class="btn btn-success">Todo Sudah Selesai
                            !</button></a>
                    <a href="/mytodo/<%= datas.id %> "><button class="btn btn-warning">Edit Todo
                            !</button></a>
                </td>
            </tr>
            <% }) %>
    </tbody>
</table>